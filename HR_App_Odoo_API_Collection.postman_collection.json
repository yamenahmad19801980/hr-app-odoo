                            "raw": "{{baseUrl}}/web",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "web"
                            ]
                        },
                        "description": "Get initial web session cookie for Odoo.sh compatibility"
                    },
                    "response": []
                },
                {
                    "name": "2. Authenticate User",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json",
                                "type": "text"
                            },
                            {
                                "key": "User-Agent",
                                "value": "HR App Flutter Odoo.sh",
                                "type": "text"
                            },
                            {
                                "key": "Accept",
                                "value": "application/json",
                                "type": "text"
                            },
                            {
                                "key": "Cookie",
                                "value": "session_id={{sessionId}}",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"jsonrpc\": \"2.0\",\n  \"method\": \"call\",\n  \"params\": {\n    \"db\": \"{{database}}\",\n    \"login\": \"{{username}}\",\n    \"password\": \"{{password}}\"\n  }\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/web/session/authenticate",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "web",
                                "session",
                                "authenticate"
                            ]
                        },
                        "description": "Authenticate user with Odoo credentials and get session"
                    },
                    "response": []
                },
                {
                    "name": "3. Test Connection",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "User-Agent",
                                "value": "HR App Flutter Odoo.sh",
                                "type": "text"
                            }
                        ],
                        "url": {
                            "raw": "{{baseUrl}}/web",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "web"
                            ]
                        },
                        "description": "Test if Odoo server is reachable"
                    },
                    "response": []
                }
            ],
            "description": "Authentication and session management endpoints"
